{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./typings",
      "declaration": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
        "dom",
        "es2015"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "target": "es5",
      "skipLibCheck": true,
      "stripInternal": true,
      "noImplicitAny": false,
      "noResolve": true,
      "typeRoots": [
        "node_modules/@types/"
      ]
    },
    "exclude": [
      "node_modules",
    ],
    "compileOnSave": false
  }